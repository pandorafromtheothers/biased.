{
    "name": "GET YT Music Tracklist",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "js": ["main.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "action": {
        "default_icon": "logo.png",
        "default_title": "Open Index"
    }
}